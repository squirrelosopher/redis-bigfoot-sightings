{
  "name": "redis-bigfoot-sightings",
  "version": "1.0.0",
  "private": true,
  "description": "Using RediSearch and RedisJSON, Redis modules, to provide powerful search over Bigfoot sightings.",
  "main": "./src/server.js",
  "type": "module",
  "scripts": {
    "data:load": "node ./load_data.js",
    "data:clear": "node ./clear_data.js",
    "open:page": "node ./open_url.js",
    "compile:clean": "rimraf public/scripts/dist/",
    "compile:dev": "npm run compile:clean && cross-env NODE_ENV=development node ./esbuild.js",
    "compile:prod": "npm run compile:clean && cross-env NODE_ENV=production node ./esbuild.js",
    "server:start:dev": "npm run compile:dev && npm run open:page && node ./bin/www.js",
    "server:start:prod": "npm run compile:prod && node ./bin/www.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/squirrelosopher/redisearch-bigfoot-sightings.git"
  },
  "bugs": {
    "url": "https://github.com/squirrelosopher/redisearch-bigfoot-sightings/issues"
  },
  "homepage": "https://github.com/squirrelosopher/redisearch-bigfoot-sightings#readme",
  "keywords": [
    "RediSearch",
    "RedisJSON",
    "Redis",
    "Plotly",
    "Bigfoot"
  ],
  "dependencies": {
    "axios": "^0.26.0",
    "cookie-parser": "~1.4.4",
    "csv-parser": "^3.0.0",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "ioredis": "^4.28.5",
    "morgan": "~1.9.1",
    "open": "^8.4.0",
    "regenerator-runtime": "^0.13.9"
  },
  "author": "Aleksandar MiladinoviÄ‡",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.25",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2"
  }
}
